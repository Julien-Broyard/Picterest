{
    "name": "picterest/picterest",
    "type": "project",
    "description": "Simple Pinterest clone made with Symfony 5.",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "easycorp/easyadmin-bundle": "^3.2",
        "knplabs/knp-paginator-bundle": "^5.4",
        "knplabs/knp-time-bundle": "^1.15",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-client": "5.2.*",
        "symfony/intl": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/messenger": "5.2.*",
        "symfony/mime": "5.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.2.*",
        "symfony/process": "5.2.*",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.2.*",
        "symfony/string": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/twig-bundle": "^5.2",
        "symfony/uid": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/web-link": "5.2.*",
        "symfony/yaml": "5.2.*",
        "symfonycasts/reset-password-bundle": "^1.4",
        "symfonycasts/verify-email-bundle": "^1.2",
        "twig/extra-bundle": "^2.12 || ^3.0",
        "twig/string-extra": "^3.3",
        "twig/twig": "^2.12 || ^3.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.13",
        "friendsofphp/php-cs-fixer": "^2.18",
        "hautelook/alice-bundle": "^2.9",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpstan/phpstan": "^0.12.79",
        "phpstan/phpstan-doctrine": "^0.12.32",
        "phpstan/phpstan-phpunit": "^0.12.17",
        "phpstan/phpstan-symfony": "^0.12.20",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.2",
        "symfony/css-selector": "^5.2",
        "symfony/debug-bundle": "^5.2",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^5.2",
        "symfony/var-dumper": "^5.2",
        "symfony/web-profiler-bundle": "^5.2",
        "thecodingmachine/phpstan-strict-rules": "^0.12.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "format": [
            "./vendor/bin/php-cs-fixer fix src",
            "composer normalize"
        ],
        "lint": [
            "symfony console lint:yaml config --parse-tags",
            "symfony console doctrine:schema:validate --skip-sync -vvv --no-interaction",
            "symfony console lint:container --no-debug",
            "symfony console lint:twig templates --env=prod",
            "./vendor/bin/parallel-lint --exclude vendor .",
            "./vendor/bin/phpstan analyse src --level 7"
        ],
        "setup": [
            "composer install",
            "composer start",
            "symfony console doctrine:migrations:migrate --no-interaction",
            "symfony console hautelook:fixtures:load --no-interaction"
        ],
        "start": [
            "docker-compose up -d",
            "symfony serve -d",
            "symfony run -d --watch=config,src,templates,vendor symfony console messenger:consume async"
        ],
        "stop": [
            "docker-compose stop",
            "symfony server:stop",
            "symfony console messenger:stop-workers"
        ],
        "test": [
            "php vendor/bin/simple-phpunit -v"
        ]
    }
}
